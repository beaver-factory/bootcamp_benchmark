{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 30 7 * * 1-5"
    },
    {
      "name": "inBlob",
      "type": "blob",
      "direction": "in",
      "path": "%BlobContainer2%/latest_course_report_skills.csv",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer1%/adzuna_raw_{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
