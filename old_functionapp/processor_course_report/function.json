{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%BlobContainer1%/course_report_raw_{timestamp}.json",
      "connection": "AzureWebJobsStorage",
      "dataType": "string"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer2%/course_report_courses_{DateTime}.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outBlob2",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer2%/course_report_skills_{DateTime}.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outBlob3",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer2%/latest_course_report_skills.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
