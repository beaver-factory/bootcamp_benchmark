{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%BlobContainer1%/CourseReport_{timestamp}.json",
      "connection": "AzureWebJobsStorage",
      "dataType": "string"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer2%/processed_course_data_{DateTime}.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outBlob2",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer2%/processed_skills_data_{DateTime}.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
