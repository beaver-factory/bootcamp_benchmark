{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */60 * * * *"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainerPrefix%0/Adzuna_{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}