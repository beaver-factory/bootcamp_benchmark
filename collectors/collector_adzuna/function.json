{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 30 11 * * TUE,FRI"
    },
    {
      "name": "inSkillsDict",
      "type": "blob",
      "direction": "in",
      "path": "%BlobContainer3%/skills_dict.json",
      "connection": "AzureWebJobsStorage",
      "dataType": "binary"
    },
    {
      "name": "outAdzunaJobCounts",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainer1%/adzuna_raw_{DateTime}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
