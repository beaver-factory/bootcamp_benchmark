{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%BlobContainerPrefix%0/{blobName}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BlobContainerPrefix%1/ab.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
